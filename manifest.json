{
  "manifest_version": 3,
  "name": "ContactOut Import Tool",
  "version": "1.0.0",
  "description": "Adds import functionality to ContactOut search results with React UI",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://contactout.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://contactout.com/dashboard/search*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "ContactOut Import Tool"
  },
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["https://contactout.com/*"]
    }
  ]
} 